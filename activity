<script>
var len=$(".box li").length;
function int(){
for(i=8;i<len;i++){
$(".box li").eq(i).addClass("hid");   
}
}
$(".tog").click(function(){
var t=$(this).text();
if(t=="展开"){
$(".box .hid").addClass("show");
$(this).text("收缩");
}else{
$(".box .hid").removeClass("show");
$(this).text("展开");
}  
});
int();//初始化隐藏多余标签
</script>
